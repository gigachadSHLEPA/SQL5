<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes">
    <Style TargetType="TextBlock" x:Key="TxtFont1">
        <Setter Property="TextElement.FontFamily" Value="/fonts/#Retro Typo"/>
    </Style>
    <Style TargetType="TextBox" x:Key="TxtFont2">
        <Setter Property="TextElement.FontFamily" Value="/fonts/#Retro Typo"/>
    </Style>
    <Style TargetType="TextBox" x:Key="TxtFont3" BasedOn="{StaticResource MaterialDesignTextBoxBase}">
        <Setter Property="FontFamily" Value="/fonts/#Ankh Sanctuary"/>
    </Style>
    <Style TargetType="TextBlock" x:Key="TxtFont4">
        <Setter Property="TextElement.FontFamily" Value="/fonts/#mr_HamiltoneG"/>
    </Style>

    <Style TargetType="PasswordBox" x:Key="TxtFont5" BasedOn="{StaticResource MaterialDesignPasswordBox}">
        <Setter Property="FontFamily" Value="/fonts/#Ankh Sanctuary"/>
    </Style>

    <Style TargetType="Button" x:Key="TxtFont6" BasedOn="{StaticResource MaterialDesignRaisedButton}">
        <Setter Property="FontFamily" Value="/fonts/#Heart Display"/>
    </Style>
    
</ResourceDictionary>
